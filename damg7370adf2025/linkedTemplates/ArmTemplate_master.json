{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "damg7370adf2025"
		},
		"STG_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'STG_AzureDataLakeStorage'"
		},
		"Snowflake_demo_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Snowflake_demo'"
		},
		"AzureDataLakeStorage_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://damg7370spring.dfs.core.windows.net/"
		},
		"AzureKeyVault_LS_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://damg7370kv2025.vault.azure.net/"
		},
		"LS_Stg_AzureBlobStorage_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'LS_Stg_AzureBlobStorage'"
		},
		"STG_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://damg7370springstg.dfs.core.windows.net/"
		},
		"Snowflake_demo_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "TEMP_DB"
		},
		"Snowflake_demo_properties_typeProperties_role": {
			"type": "string",
			"defaultValue": "DAMG_ROLE"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "damg7370adf2025_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"STG_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('STG_AzureDataLakeStorage_accountKey')]"
					},
					"Snowflake_demo_password": {
						"value": "[parameters('Snowflake_demo_password')]"
					},
					"AzureDataLakeStorage_LS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_LS_properties_typeProperties_url')]"
					},
					"AzureKeyVault_LS_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_LS_properties_typeProperties_baseUrl')]"
					},
					"LS_Stg_AzureBlobStorage_sasUri": {
						"value": "[parameters('LS_Stg_AzureBlobStorage_sasUri')]"
					},
					"STG_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('STG_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"Snowflake_demo_properties_typeProperties_database": {
						"value": "[parameters('Snowflake_demo_properties_typeProperties_database')]"
					},
					"Snowflake_demo_properties_typeProperties_role": {
						"value": "[parameters('Snowflake_demo_properties_typeProperties_role')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}